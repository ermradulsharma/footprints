{
    "name": "skywalker-labs/footprints",
    "description": "Seamlessly track UTM parameters and referrers for user registration attribution in Laravel applications.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "utm",
        "attribution tracking",
        "visitor footprints",
        "marketing analytics",
        "user registration",
        "skywalker"
    ],
    "homepage": "https://github.com/skywalker-labs/footprints",
    "support": {
        "issues": "https://github.com/skywalker-labs/footprints/issues",
        "source": "https://github.com/skywalker-labs/footprints"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ermradulsharma"
        }
    ],
    "authors": [
        {
            "name": "Skywalker Labs Team",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://skywalker-labs.com/",
            "role": "Developer"
        },
        {
            "name": "Mradul Sharma",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://mradulsharma.vercel.app/",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "jaybizzle/crawler-detect": "^1.2",
        "ermradulsharma/laravel-toolkit": "^1.0|^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.3|^10.0|^11.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Skywalker\\Footprints\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skywalker\\Footprints\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Skywalker\\Footprints\\FootprintsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}